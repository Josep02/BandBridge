{% extends 'base_back.html.twig' %}

{% block title %}Musician{% endblock %}

{% block body %}
<div class="rounded-2 bg-white p-3">
    <h1>Musician</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ musician.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ musician.name }}</td>
        </tr>
        <tr>
            <th>Lastname</th>
            <td>{{ musician.lastname }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ musician.username }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ musician.email }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ musician.image }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ musician.password }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_musician_index') }}">back to list</a>

    <a href="{{ path('app_back_musician_edit', {'id': musician.id}) }}">edit</a>

    {{ include('back_musician/_delete_form.html.twig') }}
</div>
{% endblock %}
