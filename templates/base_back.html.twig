<!DOCTYPE html>
<html lang="ca">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}BandBridge{% endblock %}</title>
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% include '_header_back.html.twig' %}
<main class="container-fluid">
    <div class="row">
        {% include '_aside_back.html.twig' %}
        <section class="col-lg-10 col-md-12 bg-2 p-2">
            {% block body %}
            {% endblock %}
        </section>
    </div>
</main>
{% include '_footer_back.html.twig' %}
</body>
</html>